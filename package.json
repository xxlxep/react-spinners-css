{
  "name": "react-spinners-css",
  "version": "1.0.0",
  "description": "React css-only spinners",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js",
    "lint:fix": "pretter-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-comit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxlxep/react-spinners-css.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "Andy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xxlxep/react-spinners-css/issues"
  },
  "homepage": "https://github.com/xxlxep/react-spinners-css#readme",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.2",
    "rollup-plugin-node-resolve": "^5.0.1"
  },
  "dependencies": {
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7"
  }
}
