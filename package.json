{
  "name": "react-spinners-css",
  "version": "1.0.0",
  "description": "React css-only spinners",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js",
    "lint:fix": "pretter-eslint '**/*.js' --write",
    "storybook": "start-storybook -p 9001 -c storybook",
    "storybook:build": "build-storybook -c storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stagged": "jest --findRelatedTests"
  },
  "husky": {
    "hooks": {
      "pre-comit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test:stagged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxlxep/react-spinners-css.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "Andy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xxlxep/react-spinners-css/issues"
  },
  "homepage": "https://github.com/xxlxep/react-spinners-css#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.0.0"
  },
  "dependencies": {
    "@storybook/addon-options": "^5.1.3",
    "husky": "^2.3.0",
    "lint-staged": "^13.1.0"
  },
  "peerDependencies": {
    "react": "16.7.0"
  },
  "engines": {
    "node": ">8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/mocks/styles.js"
    }
  }
}
